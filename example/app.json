{
  "expo": {
    "name": "react-native-workouts-example",
    "slug": "react-native-workouts-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "expo.modules.workouts.example",
      "infoPlist": {
        "NSHealthShareUsageDescription": "This app needs access to health data to sync workouts.",
        "NSHealthUpdateUsageDescription": "This app needs to write workout data."
      }
    },
    "plugins": [],
    "android": {
      "package": "expo.modules.workouts.example"
    }
  }
}